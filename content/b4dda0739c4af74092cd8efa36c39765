<script type="text/javascript" language="javascript">
function CheckKey(e)
{
	var unicode = e.keyCode ? e.keyCode : e.charCode;
	if(unicode == 13)
	{
	    submitForm();
        return false;
	}
	return true;
}
var btn = document.getElementById('ctl05_btn');
function submitForm()
{
    var q = document.getElementById('ctl05_q').value;
    var startat = document.getElementById('ctl05_startat').value;
    window.location = '/SearchResults/?startat='+startat+'&amp;q='+q;
}
btn.onclick = submitForm;
</script>

