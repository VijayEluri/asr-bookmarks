<script type="text/javascript">
	var Container = $('hostel-map');
	var ContainerSize = Container.getSize();
	var ContainerPos = Container.getPosition();
	var CordsElement = Container.getElement('.cords');
	var NameElement = Container.getElement('.name');
	var Points = [];
	var i = 0;
	
	
	
	var init = function(){
		for( var k = 0; k < Points.length; k++ )
			addPoint( Points[k] );
	}
	
	var addPoint = function(data){
		var x = ((ContainerSize.x/100)*data.x)-5.5;
		var y = ((ContainerSize.y/100)*data.y)-5.5;
		
		var point = new Element('a', {
			'class': 'point',
			'href': data.link,
			'styles': {
				'left': x,
				'top': y
			},
			'events': {
				'mouseenter': function(e){
					NameElement.set('html', data.name);
					NameElement.setStyles({
						'opacity': 0.75,
						'left': x,
						'top': y - NameElement.getHeight()-2
					});
				}.bind(this),
				'mouseleave': function(e){
					NameElement.setStyles({
						'opacity': 0,
						'visibility': 'hidden'
					});
				}.bind(this)
			}
		});
		
		point.inject( Container );
	}
	
	// Add Hostels to map
	Points[i++] = { x: 23, y: 73, name: 'Reykjav&iacute;k Downtown', link: '/Hostels/Reykjavikdowntown/' };
Points[i++] = { x: 9, y: 30, name: 'B&iacute;ldudalur', link: '/Hostels/Bildudalur/' };
Points[i++] = { x: 38, y: 96, name: 'Vestmannaeyjar', link: '/Hostels/Vestmannaeyjar/' };
Points[i++] = { x: 16, y: 80, name: 'Njar&eth;v&iacute;k', link: '/Hostels/Njardvik/' };
Points[i++] = { x: 14, y: 19, name: 'Korpudalur', link: '/Hostels/Korpudalur/' };
Points[i++] = { x: 20, y: 31, name: 'Reykh&oacute;lar', link: '/Hostels/Reykholar/' };
Points[i++] = { x: 29, y: 30, name: 'Broddanes', link: '/Hostels/Broddanes/' };
Points[i++] = { x: 30, y: 37, name: 'S&aelig;berg', link: '/Hostels/Saeberg/' };
Points[i++] = { x: 35, y: 32, name: '&Oacute;sar', link: '/Hostels/Osar/' };
Points[i++] = { x: 50, y: 15, name: 'Siglufj&ouml;r&eth;ur', link: '/Hostels/Siglufjordur/' };
Points[i++] = { x: 57, y: 27, name: 'Akureyri', link: '/Hostels/Akureyri/' };
Points[i++] = { x: 61, y: 20, name: 'Berg', link: '/Hostels/Berg/' };
Points[i++] = { x: 71, y: 10, name: 'K&oacute;pasker', link: '/Hostels/Kopasker/' };
Points[i++] = { x: 82, y: 8, name: 'Ytra L&oacute;n', link: '/Hostels/Ytralon/' };
Points[i++] = { x: 90, y: 27, name: 'H&uacute;sey', link: '/Hostels/Husey/' };
Points[i++] = { x: 96, y: 30, name: 'Borgarfj&ouml;r&eth;ur Eystri', link: '/Hostels/Borgarfjordureystri/' };
Points[i++] = { x: 93, y: 38, name: 'Sey&eth;isfj&ouml;r&eth;ur', link: '/Hostels/Seydisfjordur/' };
Points[i++] = { x: 90, y: 57, name: 'Berunes', link: '/Hostels/Berunes/' };
Points[i++] = { x: 85, y: 68, name: 'H&ouml;fn', link: '/Hostels/Hofn/' };
Points[i++] = { x: 78, y: 72, name: 'Vagnsstadir', link: '/Hostels/Vagnsstadir/' };
