function checkform(PRODProducts){
var formcolor = document.getElementById('theshirtcolor');
var formsize = document.getElementById('theshirtsize');
if (formcolor.value=="") {
alert("Please choose a color.");
return false;}
else if(formsize.value==""){
alert("Please choose a size.");
return false;}
else {
return true;
}}
document.forms[1].onsubmit = checkform;
