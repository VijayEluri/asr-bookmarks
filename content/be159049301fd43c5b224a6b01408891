/* CBS radio full page player - updated 8/31/10 */
function playrad(StationID) {
    if (StationID == null || StationID == "0") {
        StationID = '';
    }
    var urlStr = 'http://player.radio.com/player/AOLPlayer.php?version=1.1&station=' + StationID;
    var features = ""; //"width=800,height=600, border=0, status=no, resizable=no ,scrollbars=no, location=no, menubar=no,toolbar=no, left=200, top=200";
    var oWindow = window.open(urlStr, "AOLPlayer", features, true);
    oWindow.focus();
}
