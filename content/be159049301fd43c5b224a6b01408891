    <script type="text/javascript">
        // JS Vars normally set for Portal Supertab Carousel
var carouselSpeed=600;
var carouselScroll=4;
var carouselEasing="jswing";


// Imported Socnet JS
var socnet = {
    PAGE_SIZE : 3,
    loadNetwork : function( network, options, isTab ) {
        if (isTab == null) {
            isTab = false;
        }
        if ($("#socnet_content_wrap > #" + network + ".tab").length == 1) {
            options = $.extend({useCache : true, post:null, action:null}, options);
            if ($("#socnet_content_wrap > div#" + network + " ul.friends").length > 0 && options.useCache) {
                $("#socnet_content_wrap > div.tab").hide().filter("#" + network).show();
            } else {
                $("#socnet_content_wrap > div.tab").hide().filter("#socnet-loader").show().addClass(network);
                $("#socnet_content_wrap > #" + network + ".tab").load("/__socnet/ajax/" + network + ".html" + (options.action != null ? "?" + options.action + "&tab=" + isTab : "?tab=" + isTab), options.post, function() {
                    if ($(this).find("ul.friends").length > 0) {
                        $(this).find("ul.friends > li:gt(2)").hide().end().find(".pagination-links > div > a.number").click(function() {
                            var p = parseInt($(this).text()), size = parseInt(this.rel);
                            $(this).parents("div.tab").find("ul.friends").find("li").show().end().find("li:lt(" + ((p - 1) * size) + ")").hide().end().find("li:gt(" + ((p * size) - 1 ) + ")").hide();
                            $(this).addClass("selected").siblings("a.selected").removeClass("selected");
                            if ($(this).next(".number").length == 0) {
                                $(this).siblings(".button_next").hide();
                            } else {
                                $(this).siblings(".button_next").show();
                            }
                            if ($(this).prev(".number").length == 0) {
                                $(this).siblings(".button_previous").hide();
                            } else {
                                $(this).siblings(".button_previous").show();
                            }
                            return false;
                        }).end().find(".button_next").click(function() {
                            $(this).siblings("a.selected").next("a.number").click();
                        }).end().find(".button_previous").click(function() {
                            $(this).siblings("a.selected").prev("a.number").click();
                        }).end().find(".info_header > a").click(function() {
                            var $parent = $(this).parents("div:eq(1)");
                            $parent.find("div.friend-image > a").unbind("mouseover").unbind("mouseout");
                            $parent.toggleClass("area_friends_listview").toggleClass("area_friends_thumbview");
                            $(this).parents("div.tab").find(".pagination-links > div." + $parent.get(0).className.replace("area_friends_", "").replace("view", "") + " > a.selected").click();
                            if ($parent.hasClass("area_friends_thumbview")) {
                                $parent.find("div.friend-image > a > img").mouseover(function() {
                                    if ($("#" + network + " .tooltip").length <= 0) {
                                        $("#" + network + " .area_friends_thumbview").append('<div class="friend-info tooltip">Tooltip</div>');
                                    }
                                    $("#" + network + ' .tooltip').empty().append($(this).parent().parent().siblings("div.friend-info").clone().children());
                                    var this_offset = $(this).offset(), container_offset = $("#" + network + " .area_friends_thumbview").offset(), tooltip_width = $("#" + network + " .tooltip").width(), container_offset_width = $("#" + network + " .area_friends_thumbview").width(), tooltip_margin = 20, _top = this_offset.top - container_offset.top + tooltip_margin;
                                    if (((this_offset.left - container_offset.left) + tooltip_width + tooltip_margin) > container_offset_width) {
                                        var _left = container_offset_width - tooltip_width - tooltip_margin;
                                    } else {
                                        var _left = this_offset.left - container_offset.left + tooltip_margin;
                                    }
                                    $("#" + network + ' .tooltip').css('position', 'absolute').css('top', _top).css('left', _left).show().mouseover(function() {
                                        $(this).show();
                                    }).mouseout(function() {
                                        $(this).hide();
                                    });
                                }).mouseout(function( e ) {
                                    $("#" + network + ' .tooltip').hide();
                                    var relTarg = e.relatedTarget || e.fromElement, info = $(this).parent().siblings("div.friend-info");
                                    if (relTarg != info.get(0)) {
                                        info.css({display: ""});
                                    }
                                });
                            } else {
                                $parent.find("div.friend-image > a").unbind("mouseover").unbind("mouseout");
                            }
                            return false;
                        }).end().find(".greeting").find("a.logout").click(function() {
                            socnet.logout(network);
                            return false;
                        }).end().find("a.refresh").click(function() {
                            socnet.loadNetwork(network, {useCache:false});
                            return false;
                        });
                        $("#global_status_controls").show().find("span." + network).parent().removeClass("disabled");
                    } else {
                        $("ul#sncs > li." + network + " > a > span").html("(login)").removeClass("red");
                        $("#global_status_controls").show().find("span." + network).parent().addClass("disabled");
                        if ($("#module_socnet #global_status_controls a").not("a.disabled").not("a.submit").length == 0) {
                            $("#global_status_controls").hide().find("span.networks > a").removeClass("inactive").addClass("disabled");
                        }
                    }
                    $("#socnet_content_wrap > div.tab").hide().filter("#" + network).show().end().filter("#socnet-loader").removeClass(network);
                });
            }
            var date = new Date();
            date.setTime(date.getTime() + (14 * 24 * 60 * 60 * 1000));
            document.cookie = "socnet_an=" + network + ";expires=" + date.toGMTString();
        }
    },
    refreshAll : function() {
        var network = $("#socnet_content_wrap > div.tab:visible").get(0).id;
        $("#socnet_content_wrap > div.tab").not("#socnet-loader").empty();
        this.loadNetwork(network);
    },
    logoutAll : function() {
        $.getJSON("/__socnet/ajax/all.html?logout", function() {
            $("ul#sncs > li > a > span").html("(login)").removeClass("red");
            $("#global_status_controls").hide().find("span.networks > a").removeClass("inactive").addClass("disabled");
            socnet.refreshAll();
        });
    },
    activeNetworks : function() {
        return $("#module_socnet #global_status_controls a").not("a.submit").not("a.disabled").not("a.inactive").children("span").map(function() {
            return this.className;
        }).get();
    },
    logout : function( network ) {
        socnet.loadNetwork(network, {action:"logout", useCache:false});
        $("ul#sncs > li." + network + " > a > span").html("(login)").removeClass("red");
        $("#global_status_controls").show().find("span." + network).parent().addClass("disabled");
        if ($("#module_socnet #global_status_controls a").not("a.disabled").not("a.submit").length == 0) {
            $("#global_status_controls").hide().find("span.networks > a").removeClass("inactive").addClass("disabled");
        }
    }
};

setTimeout(function(){
$("#sncs").jcarousel({scroll: carouselScroll,speed: carouselSpeed, start: ((typeof socnet_start != "undefined") ? socnet_start : 1)}).find("li").click(function() {
    socnet.loadNetwork($(this).children()[0].name,null,true);
    $(this).addClass("selected").siblings("li").removeClass("selected");
    return false;
});
}, 1);

$(function() {
    $("#module_socnet #global_status_controls a").click(function() {
        $(this).toggleClass("inactive");
        return false;
    }).parents("#global_status_controls").find("form").submit(function() {
        var status = $("#global_status_controls form > input:text").val(), networks = socnet.activeNetworks(), i = 0, $elem = $("#module_socnet #global_status_controls small");
        $elem.siblings("div.error").remove();
        function updateStatus( network ) {

            var erIdResults = document.cookie.match ('(^|;) ?erId=([^;]*)(;|$)');
            if (erIdResults)
                //var erId = (encodeURIComponent(erIdResults[2]));
                var erId = (erIdResults[2]);
            else
                var erId = "";

            $elem.html("Updating: " + network);
            setTimeout(function() {
                //$elem.load("/__socnet/ajax/" + network + ".html?update", {status : status}, function() {
                //$elem.load("/__socnet/ajax/" + network + ".html?update&erId=" + erId + "&status=" + encodeURIComponent(status), function() {
                //$elem.load("/__socnet/ajax/" + network + ".html?update&erId=" + erId, {status : status}, function() {
                $elem.load("/__socnet/ajax/" + network + ".html?update", { erId : erId, status : status}, function() {
                    if ($.trim($(this).html()).length > 0) {
                        $elem.after($("<div class=\"error\"/>").html($(this).html()));
                    }

                    if (++i < networks.length) {
                        updateStatus(networks[i]);
                    } else {
                        $(this).html("All statuses updated.");
                        socnet.refreshAll();
                    }
                });
            }, 500);
        }
        if (networks.length > 0) {
            updateStatus(networks[i]);
        } else {
            $elem.html("No networks to update.");
        }
        return false;
    }).find("input:text").focus(function() {
        $(this).val("");
    }).end().find("a.submit").click(function() {
        $(this).parents("form:eq(0)").submit();
    });
});
    </script>



