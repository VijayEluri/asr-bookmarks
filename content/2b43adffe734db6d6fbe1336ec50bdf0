          <script>
function updateRoi(reset) {
   $("body").css("cursor", "progress");
   $("#roi-container").fadeTo("slow", 0.3)

   var url = 'roi_results.php';
   if (reset)
    url = 'roi.php';


   var payLoad = {
         noDevelopers:$("input[name$=noDevelopers]").val(),
         hourlyWage:$("input[name$=hourlyWage]").val(),
         redeployTime:$("input[name$=redeployTime]").val(),
         noRestarts:$("input[name$=noRestarts]").val(),
         currency:$("#currency").val()
   }
   $.ajax({
      type: "POST",
      url: "http://www.zeroturnaround.com/wp-content/themes/zeroturnaround4.0/custom/"+url,
      data: payLoad,
      success: function(msg) {
        $("body").css("cursor", "auto");
        $("#roi-container").fadeTo("slow", 1.0)
        $("#roi-container").attr('innerHTML', msg);
      }
   });
}
</script>
            <form method="POST" onSubmit="try{updateRoi();}catch(e){};return false;">
