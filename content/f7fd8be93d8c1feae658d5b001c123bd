<script type="text/javascript" language="JavaScript">
    if (document.layers) {
        document.captureEvents(Event.KEYDOWN);
    }

    document.onkeydown = function (evt) {
        var keyCode = document.layers ?
            evt.which : document.all ? event.keyCode : document.getElementById ? evt.keyCode : 0;
        if (keyCode == 27) { // esc - close recent history
            var history = document.getElementById('jive-history');
            if (history.style.display != "none") {
                new Effect.toggle('jive-history', 'blind', {duration: 0.2});
            }
        }
    }

    document.body.onclick  = function (evt) {
        var history = document.getElementById('jive-history');
        if (history.style.display != "none") {
            new Effect.toggle('jive-history', 'blind', {duration: 0.2});
        }
    }

    var elemx = 0;
    var elemy = 0;
    function XYpos(event){
        // do not set the position of the element more than once
        if (elemx == 0 && elemy == 0) {
            if (event.pageX || event.pageY) {
                elemx = window.innerWidth - event.pageX - 20;
                elemy = event.pageY + 10;
            }
            else if (event.clientX || event.clientY) {
                elemx = document.body.offsetWidth - event.clientX - 20;
                elemy = event.clientY + 10;
            }
            var elem = document.getElementById('jive-history');
            elem.style.right = elemx;
            elem.style.top = elemy;
        }
    }
</script>




