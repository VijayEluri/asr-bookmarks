<script type="text/javascript">
//<![CDATA[
	var QUOTE_COUNT = 9;
	var QUOTE_FADE_TIME = 1;
	var QUOTE_INTERVAL = 20;
	var _current_quote = 0;
	var _quote_ani;

	function postload() {
		//makebutton( 'box1-button', "Click to Learn More", function(){window.location='/zettabits/';} );
		makebutton( 'box-hw-button', "Learn More", function(){window.location='/zettabits/';} );
		makebutton( 'box-protect-button', "Get Zettabits Today", function(){window.location='/plans/';} );
		makebutton( 'box6a-button', "Click to Learn More", function(){window.location='/zettabits-zbox/';} );
		makebutton( 'box6b-button', "Click to Learn More", function(){window.location='/zettabits-backup/';} );
		makebutton( 'box6c-button', "Click to Learn More", function(){window.location='/zettabits-security/';} );
		
		_quote_ani = new SEK.Animator.Vec3( on_quote_ani, [100,0,0], 100/QUOTE_FADE_TIME );
		SEK.Timer.StartRepeating( next_quote, QUOTE_INTERVAL );
	}
	
	function next_quote() {
		_quote_ani.MoveTo( [0,0,0] );
		SEK.Timer.Start( on_swap_quote, QUOTE_FADE_TIME );
	}

	function on_quote_ani( val ) {
		var e = document.getElementById( 'quote-box' );
		var E = new SEK.Element.Wrapper(e);
		E.SetOpacity( val[0] / 100.0 );
	}
	
	function on_swap_quote() {
		// hide currently active
		var e = document.getElementById( 'quote'+String(_current_quote) );
		e.style.display = 'none';
	
		// move next
		_current_quote += 1;
		_current_quote %= QUOTE_COUNT;
		
		// show next active
		var e = document.getElementById( 'quote'+String(_current_quote) );
		e.style.display = 'block';

		// fade in
		_quote_ani.MoveTo( [100,0,0] );
	}
//]]>
</script>


