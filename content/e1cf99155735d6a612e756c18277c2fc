<script type="text/javascript">
function populaterates(){

var cdratevar = '0'; 
var spratevar = '';
var proddesccdvar='';
var cdminamtvar='';
var effdatevar ='';
var cdeffdatevar ='';

for (var i = 0; i < depositProducts.length; i++) {
  if (depositProducts[i].type == "cd") {
  for (var j = 0; j < depositProducts[i].rates.length; j++) {
  	if (depositProducts[i].rates[j].display == "3" ) {
    cdratevar = depositProducts[i].rates[j].formattedAPY;
    proddesccdvar = depositProducts[i].description;
    cdminamtvar = depositProducts[i].rates[j].formattedMinAmount;
    	var cdefd = depositProducts[i].rates[j].startDate;
    	cdeffdatevar = cdefd.substring(5,10)+'/'+cdefd.substring(0,4);    
    }
   }
  }
}

for (var i = 0; i < depositProducts.length; i++) {
  	if (depositProducts[i].name == "savingsplus") {
  		for (var j = 0; j < depositProducts[i].rates.length; j++) {
  	   if (depositProducts[i].rates[j].formattedMinAmount == "100,000") {
    	spratevar = depositProducts[i].rates[j].formattedAPY;
    	var efd = depositProducts[i].rates[j].startDate;
    	effdatevar = efd.substring(5,10)+'/'+efd.substring(0,4);    	
    	}
  	}
  }
}


  if (cdratevar != '0') {
		document.getElementById("cdrate").innerHTML = cdratevar + "%&nbsp;APY**";
		document.getElementById("cdratelink").innerHTML = "More Rates";
 } else {
 		document.getElementById("cdrate").innerHTML ="Great rates";
		document.getElementById("cdratelink").innerHTML = "View Now";
		document.getElementById("cdlegal").style.display = "none";
   }			
	document.getElementById("splusapy").innerHTML = spratevar;
	document.getElementById("proddesccd").innerHTML = proddesccdvar;
	document.getElementById("effdate1").innerHTML = cdeffdatevar;
	document.getElementById("effdate2").innerHTML = effdatevar;
	document.getElementById("cdminamt").innerHTML = cdminamtvar;
}
</script>
