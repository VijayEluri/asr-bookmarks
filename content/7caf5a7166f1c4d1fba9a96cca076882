<script type="text/javascript">
    if (top != self) {
        var isSafeTopUrl = false;
        try {
            var topUrl = top.location.href;
            var topUrlQsIdx = topUrl.indexOf('?');
            if (topUrlQsIdx > 0) {
                topUrl = topUrl.substring(0, topUrlQsIdx);
            }
            topUrl = topUrl.toLowerCase();

            var safeTopUrls = new Array(
                'http://community.jboss.org/skin-basic.jspa',
                'http://community.jboss.org/skin-advanced.jspa',
                'http://community.jboss.org/skin-advanced!UploadSkinZip.jspa',
                'http://community.jboss.org/skin-palette.jspa',
                'http://community.jboss.org/skin-palette!start.jspa'
            );
            for (var i = 0; i < safeTopUrls.length; i++) {
                if (safeTopUrls[i].toLowerCase() == topUrl) {
                    isSafeTopUrl = true;
                    break;
                }
            }
        } catch (err) {
        }
        if (!isSafeTopUrl) top.location = location;
    }
</script>

