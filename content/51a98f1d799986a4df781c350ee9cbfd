<SCRIPT LANGUAGE="JavaScript">
theimage = new Array();

// Format: theimage[...]=[name/description, image_src, linking_url]
theimage[0]=["images/woodenfence2.jpg", "#"," "];
theimage[1]=["images/hardwooddecking1.JPG", "#"," "];
theimage[2]=["images/ironfences8.jpg", "#"," "];
theimage[3]=["images/animalfence1.jpg", "#"," "];
theimage[4]=["images/ironhandrail2.jpg", "#"," "];
theimage[5]=["images/woodendeck7.jpg", "#"," "];
theimage[6]=["images/IMG_1137.jpg", "#"," "];
theimage[7]=["images/picketfences6.jpg", "#"," "];
theimage.sort(); 

///// Plugin variables

playspeed=3000;// The playspeed determines the delay for the "Play" button in ms
dotrans=1; // if value = 1 then there are transitions played in IE
transtype='blendTrans';// 'blendTrans' or 'revealtrans'
transattributes='duration=1,transition=23';// duration=seconds,transition=#<24

linksperline=4; // For the text links
playdiffernce=500; // The speed that the autplay speed is changed by
autostart=1;
preloadslides=0;
</SCRIPT>
<!-- end of configurable script -->
<!-- plugin scripts -->
