	<script type="text/javascript">

		var YUD = YAHOO.util.Dom;
		var BASE = 'http://www.clicktale.com/';

		YAHOO.util.Event.onDOMReady(function(ev) {
			CT.Base.initPanels();
			CT.ZoomEl(document.body);

			/* Search show/hide text in input field section*/
			var SearchField = YUD.get("SearchField");
			YAHOO.util.Event.on(SearchField, "focus", function() {
				if (SearchField.value == "Search") {
					SearchField.value = "";
				}
			});
			YAHOO.util.Event.on(SearchField, "blur", function() {
				if (SearchField.value == "") {
					SearchField.value = "Search";
					SearchField.style.color = "#bca9bf";
				}
			});
			YAHOO.util.Event.on(SearchField, "keyup", function() {
					SearchField.style.color = "black";
			});
			
		});

		YAHOO.util.Event.onContentReady("TopSiteNavigationYUI", function() {

			var oMenuBar = new YAHOO.widget.MenuBar("TopSiteNavigationYUI", {
				autosubmenudisplay: true,
				shadow: true, showdelay: 0, hidedelay: 100
			});
			oMenuBar.render();


		});
		
		function onMouseOverOut(cssClassToFind, hoverCssClass) {
			var el = YUD.getElementsByClassName(cssClassToFind);
			
			YAHOO.util.Event.on(el, "mouseout", function() {
				YUD.removeClass(this, hoverCssClass);
			});

			YAHOO.util.Event.on(el, "mouseover", function() {
			YUD.addClass(this, hoverCssClass);	
			});
		}
		
	</script>
	
	
	
