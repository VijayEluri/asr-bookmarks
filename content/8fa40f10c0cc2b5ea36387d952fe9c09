	<script type="text/javascript">
		// adds tags to the homepage move
		CT.MovieTaggingHelper.TagMovieStartAndEnd("HomepageVideo", "HPMovie");
		CT.MovieTaggingHelper.TagMovieTimeline("HomepageVideo", "HPMovie", 10);
	
		// tags to testimonial video
		CT.MovieTaggingHelper.TagMovieStartAndEnd("HomepageTestimonialVideo", "TMovie");
		CT.MovieTaggingHelper.TagMovieTimeline("HomepageTestimonialVideo", "TMovie", 30);

		CT.MoviePlayerHelper.addEventListener("HomepageVideo", "PLAYBACK_COMPLETED", function(obj) {
			//complete event.
		});

		var CTIsPlayback = true;
		try {
			if (!parent || !parent.WebPlayer) // if there is no WebPlayer or if the above frame is not accessible
				throw false;
		} catch (e) { CTIsPlayback = false; }
		
		function showCarousels()
		{
			function pageChangeEvtHandler(e, obj)
			{
				var pageIndex = e;
				ShowDefaultQuote(pageIndex);
			}		

			YUD.addClass(CT.GetByClass("home-grid1"), "visible");
			var ctc = new CT.Carousel("ProductTourCarousel");
			ctc.init({ animation: { speed: 0.5 }, numVisible: 5, isCircular: true });
			ctc.render();
			YUD.addClass("ProductTourCarousel", "visible");

			var ctc_customers = new CT.Carousel("CoustomersCarousel");
			ctc_customers.init({ animation: { speed: 0.5 }, numVisible: 4, isCircular: true });
			ctc_customers.render();
			YUD.addClass("CoustomersCarousel", "visible");

			var MediaBuzzCarousel = new CT.Carousel("MediaBuzzCarouselEL");
			var MediaCarousel = MediaBuzzCarousel.init({ numVisible: 1, isCircular: true });
			MediaBuzzCarousel.render();
			MediaCarousel.addListener("pageChange", pageChangeEvtHandler);

			var Resourcesarousel = new CT.Carousel("ResourcesCarouseEl");
			Resourcesarousel.init({ animation: { speed: 0.5 }, numVisible: 1, isCircular: true });
			Resourcesarousel.render();

			var Blogcesarousel = new CT.Carousel("BlogCarouseEl");
			Blogcesarousel.init({ animation: { speed: 0.5 }, numVisible: 1, isCircular: true });
			Blogcesarousel.render();
		}
		
		// execution of showCarousels is done best once the elements are available and not on DOMReady (which also includes any 3rd party analytics and services)
		//YAHOO.util.Event.onDOMReady(showCarousels);

		function isFirstOrLast(elID, index) {
			var el = YUD.get(elID);
			if (index == "0" || index == "4") {
				YUD.addClass(el, "buzz-content-first");
			}
			if (index == "3" || index == "7") {
				YUD.addClass(el, "buzz-content-last");
			}
		}
		
		
	</script>


	
	 
