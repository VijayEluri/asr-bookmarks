											<script type="text/javascript">
											/* initialize mouseOver capabilities to the logo images */
											ShowDefaultQuote(0);
											
											var buzzLogosEl = YUD.getElementsByClassName("buzz-content");
											YUE.on(buzzLogosEl, "mouseover", function() {
												//remove all selected nodes (reset view) and set current selected.
												var selected_elements = YUD.getElementsByClassName("buzz-logo-hover");
												YUD.removeClass(selected_elements, "buzz-logo-hover");
												YUD.addClass(this, "buzz-logo-hover");
												var buzzQuote = this.getElementsByTagName("em");
												var CustomerLogoQuoteWrap = YUD.get("CustomerLogoQuoteWrap");
												CustomerLogoQuoteWrap.innerHTML = buzzQuote[0].innerHTML;

											});

											var MediaBuzzContant = YUD.getElementsByClassName("MediaBuzzContant");
											YUE.on(MediaBuzzContant, "click", function() {
												window.location = BASE + "company/media_buzz";
											});
											
											/*YUE.on(buzzLogosEl,"mouseout",function(){
												YUD.removeClass(this,"buzz-logo-hover");
												YUD.get("CustomerLogoQuoteWrap").innerHTML = "";
												ShowDefaultQuote();
											});*/


											function ShowDefaultQuote(pageIndex) {
												var self = null;
												var buzzLogosEl = YUD.getElementsByClassName("buzz-content");
												if (pageIndex == 0) {
													self = buzzLogosEl[0]
												}
												else {
													self = buzzLogosEl[4]
												}
												var buzzQuote = self.getElementsByTagName("em");
												
												var selected_elements = YUD.getElementsByClassName("buzz-logo-hover");
												YUD.removeClass(selected_elements, "buzz-logo-hover");
												YUD.addClass(self, "buzz-logo-hover");

												var CustomerLogoQuoteWrap  = YUD.get("CustomerLogoQuoteWrap");
												CustomerLogoQuoteWrap.innerHTML = buzzQuote[0].innerHTML;
												/*YUE.on(CustomerLogoQuoteWrap, "click", function() {
													alert('');
												});*/
												
											}


											
											
											</script>
											
