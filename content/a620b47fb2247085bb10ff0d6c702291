function prepare_price_display(num_files,pdf_price_elem,pdf_f_price)
{
  pdf_price_elem.innerHTML = num_files+" * "+" $"+pdf_f_price+" = "+"$"+(num_files * pdf_f_price).toFixed(2) ;

}
function initialize_vars()
{
  var el_charge_price = ge('charge_price_5');
  var files_selected = getNumFilesRequestedForPdfConversion();
  if(files_selected <= 0)
    return;
  var files_to_be_charged = 0;
  i
2035
f(el_charge_price)
  {
    var free_trials_left = el_charge_price.getAttribute('num_free_trials');
    NUM_FREE_TRIALS_LEFT = 5 + '_' + getFreeTrialsGettingUsed(free_trials_left, files_selected);
    NUM_PDF_FILES_TO_CONVERT = files_selected;
  }
}

function getFreeTrialsGettingUsed(free_trials_left, num_files)
{
  if(num_files >= free_trials_left)
    return free_trials_left;
  
  if(num_files < free_trials_left)
    return num_files;
}

function get_num_files_to_be_charged(show_alert)
{
  var el_charge_price = ge('charge_price_5');
  var files_selected = getNumFilesRequestedForPdfConversion();
  var files_to_be_charged = 0;
  if(el_charge_price)
  {
    var free_trials_left = el_charge_price.getAttribute('num_free_trials');
    NUM_FREE_TRIALS_LEFT = 5 + '_' + getFreeTrialsGettingUsed(free_trials_left, files_selected);
    NUM_PDF_FILES_TO_CONVERT = files_selected;
    var is_subscribed = el_charge_price.getAttribute('is_subscribed');
    if(is_subscribed)       
     return files_to_be_charged;
    if(!is_subscribed && show_alert)
    {
      if((free_trials_left > 0) && (files_selected > free_trials_left))
      {
        if(!confirm("You have only " + free_trials_left + " free PDF conversions left. You will be charged for additional PDF conversions. Do you want to continue?"))
        {
          return 'false';
        }
      }
    }
  }
  if(files_selected >= free_trials_left)
    files_to_be_charged = (files_selected - free_trials_left);
  else
    files_to_be_charged = 0;

  return files_to_be_charged;
}


function calculate_total_price() {
  BATCH_LOCK = 0;
  TOTAL_PPU_PRICE = 0;
  PPU_PRODUCT = '';
  var files_to_be_charged = get_num_files_to_be_charged(true);
  if(files_to_be_charged == 'false')
  {
    return 'false';
  }

  //this variable holds the number of files that have to be charged for PDF conversion
  if(files_to_be_charged > 0)
    NUM_PDF_FILES_TO_CHARGE = files_to_be_charged;
  var feature = 0;
         feature = ge("fid0");
    if((feature != null) && (feature.checked || (feature.value != null && 'on' == feature.value))) {
      PPU_PRODUCT = PPU_PRODUCT+","+";"+"Premium Delivery";
      if(0 != 5)
      TOTAL_PPU_PRICE = TOTAL_PPU_PRICE + 8.99;
      else
      {
        var amount = 8.99 * files_to_be_charged;
        TOTAL_PPU_PRICE = TOTAL_PPU_PRICE + amount;
      }
      BATCH_LOCK = BATCH_LOCK  + 1;
    }
    TOTAL_PPU_PRICE = Math.floor(TOTAL_PPU_PRICE*100)/100;
           feature = ge("fid1");
    if((feature != null) && (feature.checked || (feature.value != null && 'on' == feature.value))) {
      PPU_PRODUCT = PPU_PRODUCT+","+";"+"Password-Protected Secure Delivery";
      if(1 != 5)
      TOTAL_PPU_PRICE = TOTAL_PPU_PRICE + 3.99;
      else
      {
        var amount = 3.99 * files_to_be_charged;
        TOTAL_PPU_PRICE = TOTAL_PPU_PRICE + amount;
      }
      BATCH_LOCK = BATCH_LOCK  + 2;
    }
    TOTAL_PPU_PRICE = Math.floor(TOTAL_PPU_PRICE*100)/100;
           feature = ge("fid2");
    if((feature != null) && (feature.checked || (feature.value != null && 'on' == feature.value))) {
      PPU_PRODUCT = PPU_PRODUCT+","+";"+"Certified Delivery with Tracking";
      if(2 != 5)
      TOTAL_PPU_PRICE = TOTAL_PPU_PRICE + 3.99;
      else
      {
        var amount = 3.99 * files_to_be_charged;
        TOTAL_PPU_PRICE = TOTAL_PPU_PRICE + amount;
      }
      BATCH_LOCK = BATCH_LOCK  + 4;
    }
    TOTAL_PPU_PRICE = Math.floor(TOTAL_PPU_PRICE*100)/100;
           feature = ge("fid3");
    if((feature != null) && (feature.checked || (feature.value != null && 'on' == feature.value))) {
      PPU_PRODUCT = PPU_PRODUCT+","+";"+"Return Receipt";
      if(3 != 5)
      TOTAL_PPU_PRICE = TOTAL_PPU_PRICE + 3.99;
      else
      {
        var amount = 3.99 * files_to_be_charged;
        TOTAL_PPU_PRICE = TOTAL_PPU_PRICE + amount;
      }
      BATCH_LOCK = BATCH_LOCK  + 8;
    }
    TOTAL_PPU_PRICE = Math.floor(TOTAL_PPU_PRICE*100)/100;
    }
</script>
