on hideFFSpan(ff_span_id,ff_span_name)
{
  var str = ge('ff_file_ids').value;
  var str_name = ge('ff_file_names').value;
  var str_status_name = ge('ff_status').value;

  ge(ff_span_id).style.display="none";
  str = str.replace(ff_span_id,"");
  str_name = str_name.replace(ff_span_name,"");
  str_status_name = str_status_name.replace(ff_span_name,"");

  str = str.replace(/^;+/, '');
  str = str.replace(/;+$/, '');
  str_name = str_name.replace(/^;+/, '');
  str_name = str_name.replace(/;+$/, '');
  str_status_name = str_status_name.replace(/^;+/, '');
  str_status_name = str_status_name.replace(/;+$/, '');

  arr_str = str.split(";");
  ge('ff_file_ids').value = str;
  ge('ff_file_names').value = str_name;
		ge('ff_status').value = str_status_name;

  if(str == '')
  {
    ge('ff_file_forward').value = false;
  		arr_str_length = 0;
  }
  else
  {
    arr_str_length = arr_str.length;
  }
  ge('ff_file_length').innerHTML= "&nbsp;<b>" + arr_str_length + " files </b>";

    hideFfPdfInfo(ff_span_id);
		uploadHideShow();
}

function hideFfPdfInfo(ufid) {
  if (pdf_flag()) {
    if (!parent.ge("pdf-cell").hasChildNodes()) {
      rebuild_pdf_div();
    }
    var topDiv = parent.ge('list-of-files-topdf');
    var i = 0;
    while (topDiv.hasChildNodes()) {
      var subDiv = topDiv.childNodes[i];
      if (subDiv == null) {
        break;
      }
      var innerChkBox = subDiv.childNodes[0];
      var if_id = innerChkBox.getAttribute("if_id");
      var this_if_id = "if-" + ufid;
      if (this_if_id == if_id) {
        if (topDiv.childNodes.length == 1) {
          parent.ge("fid5").checked = false;
          parent.ge("step-two-subdiv-pdf").style.display="none";
        }
        topDiv.removeChild(subDiv);
      }
      i++;

      if (checkPdfStatus(false) <= 1) {
        if (!ge("pdf-cell").hasChildNodes()) {
          rebuild_pdf_div();
        }
        var pdf_cell = ge("pdf-cell");
        var i = 0;
        while (pdf_cell.hasChildNodes()) {
          pdf_cell.removeChild(pdf_cell.childNodes[i]);
        }
        i++;
      }
    }
    remove_ff_pdf_mark(ufid);
  }
}

function uploadHideShow()
{
  var flag = true;
  var str = ge('ff_file_ids').value;
  if(str != '')
		{
    str = str.replace(/^;+/, '');
    str = str.replace(/;+$/, '');
    arr_str = str.split(";");
				if(arr_str.length >= 5)
				{
      flag = false;
				}
		}
  if (flag)
  {
				ge('step-two-div').style.display = 'block';
				ge('add_upload_span').style.display = 'block';
  }
		else
		{
				ge('step-two-div').style.display = 'none';
				ge('add_upload_span').style.display = 'none';
		}
}

