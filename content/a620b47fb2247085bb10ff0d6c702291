/**
 * Get the supported file types for PDF conversion.
 * @return Array supportedFileTypes
 */
function getSupportedFileTypes() {
    var supportedFileTypes = new Array();
    return supportedFileTypes;
}

/**
 * This function returns the number of files, the user is requesting for PDF conversion.
 * Iterates through the outer div - list-of-files-topdf, to find all the child elements.
 * If the child element is a checked checkbox, then increment the counter numPdfFilesCount
 */
function getNumFilesRequestedForPdfConversion()
{
 var numPdfFilesCount = 0;
 var topDiv = parent.ge('list-of-files-topdf');
 if(pdf_flag() && topDiv != null) {
    for(var counter= 0; counter< topDiv.childNodes.length; counter++) {
      var subDiv = topDiv.childNodes[counter];
      if (subDiv == null) {
        break;
      }
      var innerChkBox = subDiv.childNodes[0];
      if(innerChkBox == null)
        break;
      //increment the counter only if the DOM element is a checked checkbox
      if(innerChkBox.type == 'checkbox')
      {
        if(innerChkBox.checked)
    numPdfFilesCount++;     
      }
    }
  }
  return numPdfFilesCount;
}

function toggleSendContinueButton()
{
  continue_to_pay();
}

