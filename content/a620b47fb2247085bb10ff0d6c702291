<script type="text/javascript">
      var orig_value = 10000000;
  var orig_date = new Date(2009,07,07); //month is 0 - 11 in javascript
  var today = new Date();
  var one_day=1000*60*60*24;
  var day_since = Math.ceil((today.getTime() - orig_date.getTime())/(one_day));
  if(day_since<0)
    day_since = 0;
  var new_sub_count = (10000000 + day_since*8000)/1000000;
  result = new_sub_count.toFixed(1);
  if((result.substring(2,3))==0)
    result = new_sub_count.toFixed(0);
  ge('sub_count').innerHTML = result+' ';
</script>
