function show_modal_popup()
{
  var accttype = "GST";
  if(accttype == "GST")
    return false;

  // if cookie is null show the modal popup else return false;
  var cookie_value=  readCookie('no_modal');
  var sess_cookie =readCookie('browser_cookie');
  if(sess_cookie !=null || cookie_value!=null){    //  this means either session cookie or dont ask me again cookie is found to be set
    return false;
  }

  Set_Cookie('browser_cookie','dont_show','');

  YAHOO.namespace("example.container");

