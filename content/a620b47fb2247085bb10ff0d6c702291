<script LANGUAGE= "Javascript" type="text/javascript">
document.domain = "yousendit.com";



 function do_pay()
 {
   parent.cc_info.AuthorizePayment();
 }

function add_expiration()
{
  var chk = document.getElementById("file_expiry");
   var id = document.getElementById("setExpiration");
   if(chk.checked)
   {
      id.style.display='inline';
   }else {
     id.style.display='none';
//    ge("download_days").value ='100';
 //   ge("expiration_time").value ='7 DAY';
   }

}
 function do_cancel()
 {
   BATCH_LOCK = 0;
   showAdvancedOptions(false);
 }

 function success_payment()
 {
   parent.cc_info.ReadyToSend();
 }

 function showAdvancedOptions(status)
 {
        parent.window.location = 'login.php?es=115';
      init_iframe_list("upload_file_list");

    if(ge("pdf_price") && !status)
    {
      var f_price = ge("pdf_price").value;
      if(ge("pdf_feature_price"))
      {
        ge("pdf_feature_price").style.marginLeft="150px";
       ge("pdf_feature_price").innerHTML="<b style='padding-top:5px; margin-top:5px;'>Your total: $"+f_price+"</b>";
        ge("pdf_feature_price").innerHTML = f_price;
      }
    }
   if(status && validate_upload(UPLOAD_IFRAME_LIST, ge("acctType").value)) {
     if(calculate_total_price() == 'false')
     {
       return false;
     }

	 ge("upload-options").style.display = "none";
	 if (ge("step-two-subdiv-pdf"))
	   ge("step-two-subdiv-pdf").style.display = "none";
	 ge("file-expire").style.display = "none";

     ge("total-feature-amount").style.display="block";
     ge("total-feature-amount").style.marginLeft="100px";
     ge("total-feature-amount").style.marginTop="10px";
     ge("total-feature-amount").innerHTML="<b style='border-top: #000000 solid 1px; padding-top:5px; margin-top:5px;'>Your total: $"+TOTAL_PPU_PRICE+"</b>";
     ge("step-one-div").style.display="none";
     //ge("step-one-subdiv").style.display="none";
     //ge("step-three-div").style.display="none";
     //In case of safari, don't hide the file select part because safari looses its value whent its hided
     if(!is_safari) {
       ge("step-two-div").style.display="none";
       ge("upload_file_list").style.display="none";
       ge("step-two-subdiv").style.display="none";
       if(ge('ff_message'))
         ge('ff_message').style.display="none";
     }
     //ge("step-two-image").style.display="none";
     //ge("add-file").style.display="none";
     //ge("step-three-div").style.display="none";
     //ge("show-advanced").style.display="none";
     ge("credit_info").style.display="block";
     ge("credit_user_info").style.display="block";
	 ge("continue-gif").style.display="none";
     disable_adv_options(true);
   }

   if(!status){
   	 ge("upload-options").style.display = "block";
   	 if (ge("step-two-subdiv-pdf"))
       ge("step-two-subdiv-pdf").style.display = "block";
   	 ge("file-expire").style.display = "block";
					ge("step-one-div").style.display="block";
     ge("step-two-subdiv").style.display="block";
     ge("upload_file_list").style.display="block";

     if(ge('ff_message'))
       ge('ff_message').style.display="block";


	 if (ge("add-file"))
	     ge("add-file").style.display="block";
     if (ge("show-advanced"))
		 ge("show-advanced").style.display="block";

     ge("credit_info").style.display="none";
     ge("credit_user_info").style.display="none";
     ge("step-two-div").style.display="block";
     ge("step-two-subdiv").style.display="none";

     if(ge("pdf-feature"))
     {
       ge("pdf-feature").style.display="block";
       if (ge("step-two-subdiv-pdf"))
         ge("step-two-subdiv-pdf").style.display="none";
     }

     ge("send-button-div").style.display="block";
     ge("continue-gif").style.display="none";
     ge("adv-options").style.display="block";
     ge("total-feature-amount").style.display="none";

	 ge("credit_info").style.display = "none";
   ge("credit_user_info").style.display="none";

     disable_adv_options(false);
   }
 }

 function makePayment()
 {
     ge("credit_info").style.display="block";
     ge("credit_user_info").style.display="block";
 }


 function validateMaxLength()
 {
     var maxAllowedCharacters = 100;
     var counter = ge("msg").value.length;
     if(counter >= maxAllowedCharacters) {
	     alert("Maximum limit specified for message text is " + maxAllowedCharacters + " characters");
    	 ge("msg").value = ge("msg").value.substring(0, maxAllowedCharacters);
     }
 }

function getContact()
{
  ajax_getContacts();
}

function TrimString(sInString) {
  sInString = sInString.replace( /^\s+/g, "" );
  return sInString.replace( /\s+$/g, "" );
}


function displayContact(e)
{
  if(e.keyCode==8) return;
  var match_rcpt = new Array();
  var count = 0;


  for(var x = 0; x < USER_CONTACTS.length; x++)
  {
  var rcpt_array = ge('rcpt').value.split(';');
  var rcpt = TrimString(rcpt_array[rcpt_array.length-1]);
    value = USER_CONTACTS[x];
    if(value.indexOf(rcpt)==0) {
      value = value + ';';
      match_rcpt[count] = value;
      count++;
    }
  }
  if(match_rcpt.length==1) {
    var last_pos = ge('rcpt').value.lastIndexOf(';');
    var typed_rcpt = ge('rcpt').value.substr(0,last_pos+1);
    ge('rcpt').value= typed_rcpt + match_rcpt[0];
  }
}

function trimContacts()
{
  var last_pos = ge('rcpt').value.lastIndexOf(';');
  if(last_pos+1 == ge('rcpt').value.length) {
    var typed_rcpt = ge('rcpt').value.substr(0,last_pos);
    ge('rcpt').value = typed_rcpt;
  }
}

//for recipient's auto fill



function auto_fill() {
  YAHOO.example.ACJSArray = function() {
    var oACDS;
    var oAutoComp;
    return {
        init: function() {

            // Instantiate first JS Array DataSource
              oACDS = new YAHOO.widget.DS_JSArray(USER_CONTACTS);

            // Instantiate first AutoComplete
            oAutoComp = new YAHOO.widget.AutoComplete('rcpt','ContactsContainer', oACDS);
            oAutoComp.queryDelay = 0;
            oAutoComp.prehighlightClassName = "yui-ac-prehighlight";
            oAutoComp.typeAhead = true;
            oAutoComp.useShadow = true;

        },

        validateForm: function() {
            // Validate form inputs here
            return false;
        }
    };
  }();
  YAHOO.widget.AutoComplete.prototype.autoHighlight = false;
  YAHOO.widget.AutoComplete.prototype.minQueryLength = 2;
  YAHOO.widget.AutoComplete.prototype.delimChar = [',',';'];
  YAHOO.util.Event.addListener(this,'load',YAHOO.example.ACJSArray.init);
}


