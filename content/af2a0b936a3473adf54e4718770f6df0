	  		<script src="/irj/portalapps/com.sap.sdn.logon/scripts/LoginUtil.js"></script>
<script type="text/javascript">
   var cookieDomain = location.hostname.substr(location.hostname.indexOf('.'));
   var rememberMeFlagCookie = new Cookie('rememberFlag'); 

   // If we have come here from a Logout, then remove the Remember Me Flag cookie (only a flag in HTTP mode)
   if (location.search == '?logout')
   {
	  rememberMeFlagCookie.flag = ""; 
      rememberMeFlagCookie.remove('/', cookieDomain, false);
      var logout = true;
   }
	
   function checkInput() {
	  // If the user checked "Remember Me", then store the flag for future reference
      if (document.loginform.rememberme.checked)
      {
    	  rememberMeFlagCookie.flag = true;
    	  rememberMeFlagCookie.store(365, '/', cookieDomain, false);
      }
      
	  if (document.loginform.j_user.value == '')
      {
         messageElement = document.getElementById('loginboxerrormsg');
         messageElement.childNodes[0].nodeValue = 'Please enter your User ID.';
         modifyCSS('remove', messageElement, 'hidden');
         document.loginform.j_user.focus();
         return;
      }
      if (document.loginform.j_password.value == '')
      {
         messageElement = document.getElementById('loginboxerrormsg')
         messageElement.childNodes[0].nodeValue = 'Please enter your Password.';
         modifyCSS('remove', messageElement, 'hidden');
         document.loginform.j_password.focus();
         return;
      }

      submitLogonForm();
   }

   function submitLogonForm()
   {
	      if (location.hostname.indexOf('dmzwdf.sap.corp') == -1) {
	          document.loginform.action = getNextLocation("/irj/scn/logon");	   
	      } else {
	          	document.loginform.action = document.location; 
	          if (location.pathname == '/irj/scn/logonsecure')
	          	document.loginform.j_authscheme.value = "sdnsecure";
	          else
	          	document.loginform.j_authscheme.value = "sdnlow";
	      }
	      document.charset = 'ISO-8859-1'; 
	      document.loginform.submit(); // submit the logon form
   }
   
   function checkEnter(event) {
     if (!event) event = window.event;
     whichCode = (event.which) ? event.which : event.keyCode;
     if (whichCode == 13)
     {
       checkInput();
     }
   }

</script>
<div id="loginbox">
  <h4 class="boxhead">Member Login</h4>
