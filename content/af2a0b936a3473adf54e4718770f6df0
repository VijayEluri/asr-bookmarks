<script type="text/javascript">
	// If the "Remember Me" cookie flag is set, then immediately redirect to HTTPS for cookie-based login
	if (rememberMeFlagCookie && (rememberMeFlagCookie.flag == "true") && !isPageHttps() && !logout)
	{
	   var nextLocation = getNextLocation("/irj/scn/logon");
	   location.replace(nextLocation);
	}
	
	// If the currentl URL does not begin with https:// then show the Certificate Login link
	if (!isPageHttps())
	{
	   	$j('#httpscertlogin').removeAttr('style');
	}

	function certlogin()
	{
		submitLogonForm();
	}
</script>
