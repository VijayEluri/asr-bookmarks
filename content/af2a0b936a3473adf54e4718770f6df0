<script type="text/javascript">
var thisUrl = location.href;
var components = thisUrl.split("/");  // 0:http:, 1:"", 2:hostname, 3 onwards is path
var isIrj = components.length > 3 && components[3]=="irj"; 
var isLogon = components.length > 5 && components[5].indexOf("logon") == 0;
if (!(isIrj && isLogon)) {
	document.loginform.redirect.value = thisUrl;
}
</script>
