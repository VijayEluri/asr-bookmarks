        rotateFwd('slow'); 
    }
    
    function rotateFwd(speed) {
    	var nextItem = 0;
        if ( (currNewsItem + 1) != newsItemCount ) {
            nextItem = currNewsItem + 1;   
        }
        $('.quoteItem').eq(currNewsItem).fadeOut(speed, function(){
            $('.quoteItem').eq(nextItem).fadeIn(speed);
        });
        currNewsItem = nextItem;
        ntTimeout = setTimeout(function(){ rotateNewsItem(); },5000);
    }
    function rotateBack(speed) {
    	var nextItem = newsItemCount-1;
        if ( (currNewsItem - 1) != (-1) ) {
            nextItem = currNewsItem - 1;   
        }
        $('.quoteItem').eq(currNewsItem).fadeOut(speed, function(){
            $('.quoteItem').eq(nextItem).fadeIn(speed);
        });
        currNewsItem = nextItem;
        ntTimeout = setTimeout(function(){ rotateNewsItem(); },5000);
    }
    function clearNtTimer () {
    	clearTimeout(ntTimeout);
    }
    
    $('#customerQuotes #quoteItemrBack').click(function(){
    	clearNtTimer();
    	rotateBack('fast');
    	return false;
    });
    $('#customerQuotes #quoteItemFwd').click(function(){
    	clearNtTimer();
    	rotateFwd('fast'); 
    	return false;
    });
        
});
-->
</script>
	</div><!-- sidebarContent -->
	
	<div class="sidebarBottomCap"></div>	
</div> <!-- sidebar #$sideBar_Class# -->














								
				
								
																
    </div><!-- / span-6 -->


