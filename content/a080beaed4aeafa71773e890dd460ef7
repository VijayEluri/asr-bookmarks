				$('#artist-select-link').click(function() {
					$('div#artist-selector div.artist-select-content').fadeToggle('fast');
					$('#artist-select-link').toggleClass('selected');
					return false;
				});
			});
		</script>
		<script type="text/javascript">
			$("#compliance").click(function() {
				createCookie("ShureImageCompliance","true",0);
				location.reload();
				return false;
			});
		</script>				
	</body>	
</html>