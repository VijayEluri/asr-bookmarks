<script type="text/javascript">
    function LangMenuObject(){
        this.LanguageList = document.getElementById("ctl00_HeaderControl_LanguageList");
        this.LanguageLink = document.getElementById("LanguageLink");
        this.Init();
    }
    LangMenuObject.prototype.Init = function(){
        this.LanguageLink.onclick = function(){
            oLangMenu.LangMenuDisplay = true;
            oLangMenu.StopLanglinkHoverListener();
            oLangMenu.ShowLanguageMenu();
            oLangMenu.StartClickListener();
        };
        this.LanguageList.onclick = function(){oLangMenu.LangMenuClicked();};
        this.LanguageLink.onmouseover = function(){oLangMenu.HighlightLanguageLink();}
        this.StartLanglinkHoverListener();
    }
    LangMenuObject.prototype.HighlightLanguageLink = function(){
        oLangMenu.LanguageLink.className = "Hover";
    }
    LangMenuObject.prototype.ResetLanguageLink = function(){
        oLangMenu.LanguageLink.className = "";
    }
    LangMenuObject.prototype.ShowLanguageMenu = function()
    {
        oLangMenu.HighlightLanguageLink();
        window.clearTimeout(oLangMenu.HideMenuTimeOut);
        oLangMenu.LanguageList.style.display = "block";
    }
    LangMenuObject.prototype.HideLanguageMenu = function()
    {
        this.HideMenuTimeOut = window.setTimeout("oLangMenu.HideIt()", 1000);
    }
    LangMenuObject.prototype.HideIt = function()
    {
        oLangMenu.LanguageList.style.display = "none";
        oLangMenu.ResetLanguageLink();
        oLangMenu.StopClickListener();
        oLangMenu.StartLanglinkHoverListener();
    }
    LangMenuObject.prototype.StartLanglinkHoverListener = function()
    {
        if (document.addEventListener) {
            this.LanguageLink.addEventListener('mouseout', this.ResetLanguageLink, false);
        } else {
            this.LanguageLink.attachEvent('onmouseout', this.ResetLanguageLink);
        }
    }
    LangMenuObject.prototype.StopLanglinkHoverListener = function()
    {
        if (document.addEventListener) {
            this.LanguageLink.removeEventListener('mouseout', this.ResetLanguageLink, false);
        } else {
            this.LanguageLink.detachEvent("onmouseout", this.ResetLanguageLink);
        }
    }
    LangMenuObject.prototype.StartClickListener = function()
    {
        if (document.addEventListener) {
            document.addEventListener('click', oLangMenu.DocClickListener, false);
        } else {
            document.attachEvent('onclick', oLangMenu.DocClickListener);
        }
    }
    LangMenuObject.prototype.StopClickListener = function()
    {
        if (document.addEventListener) {
            document.removeEventListener('click', oLangMenu.DocClickListener, false);
        } else {
            document.detachEvent("onclick", oLangMenu.DocClickListener);
        }
    }
    LangMenuObject.prototype.LangMenuClicked = function(){
        oLangMenu.LangMenuDisplay = true;
        oLangMenu.ShowLanguageMenu();
    }
    LangMenuObject.prototype.DocClickListener = function(){
        
        if(oLangMenu.LangMenuDisplay == true){
            oLangMenu.LangMenuDisplay = false;
        }
        else{
            oLangMenu.HideIt();
        }
    }
    if(document.getElementById("ctl00_HeaderControl_Language")){
        var oLangMenu = new LangMenuObject();
    }
    
    function showHelpPanel(){
        var helpPanel = document.getElementById("HelpPanel");
        helpPanel.style.display = "block";
    }
</script>
