try {
    var agent = navigator.userAgent;
    var cities = /austin|chicago|denver|madison|milwaukee|twincities/;
    var url = window.location.href;
    if(!cities.test(url)) {
        if(/mobile=false/.test(url) || /mobile=false/.test(document.cookie)) {
            var expire = new Date();
            expire.setTime(new Date().getTime() + 3600000*24*5);
            document.cookie = "mobile=false;expires="+expire.toGMTString()+";domain=.avclub.com;path=/;";
        }
        else {
            if(agent && agent !== undefined) {
                if(/iphone|ipod|android|blackberry|iemobile|symbian/i.test(agent) && !/mobile=/.test(url)) {
                    var url = window.location.href.replace("www","mobile").replace("test","testmobile").split("?");
                    window.location = url[0] + "?mobile=true";
                }
            }
        }
    }
} catch(err) {
}
</script>




