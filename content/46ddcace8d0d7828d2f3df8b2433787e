	<script type="text/javascript">

		var switchGrid = function(new_grid, initialize) {
			if (initialize || (new_grid != selected_grid)) {
				getObject(selected_grid).style.display = 'none';
				getObject(new_grid).style.display = 'block';
				selected_grid = new_grid;
			}	
		}

		var switchTab = function(new_tab, initialize) {
			if (initialize || (new_tab != selected_tab)) {
				getObject(selected_tab).className = 'tab';
				getObject(new_tab).className = 'tab selected';
				getObject(selected_tab).getElementsByTagName("p")[0].style.backgroundPosition = 'bottom left';
				getObject(new_tab).getElementsByTagName("p")[0].style.backgroundPosition = 'top left';
				selected_tab = new_tab;
			}
		}

		var hostImages = function(response) {
			response['xaction'] = 'host_images';
			var url = Serdes.make_command_url("/arrange_process", response);
			window.location.href = url;
		}
	</script>


