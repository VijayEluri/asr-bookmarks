<td width = '640'><div style='margin: 0px 20px 0px 20px;'><script language="JavaScript" type="text/javascript" >
	<!-- HIDE

// This Script in paheshome/index1.incl
// JS Arrays for CategoryArray[ ] and TitleArray[ ] - made in format/makeindexes.incl

// Populate Array of Icons into Javascript array TheIcon
//	var TheIcon = new Array();
//	for (var i=1;i<9;i++)   {
//		TheIcon[i] = new Image;
//		TheIcon[i].src = "indeximages/" + CategoryArray[i] + "top1.png";
//	}

// Function IconRoll() Rolls Icons on Home Page
	var IconCount = 1 + (Math.floor(Math.random()*8));					// Random Choice 1 - 8
	var IconContent;															// Text, Link, and Image
	function IconRoll()   {													// Timed Rollover
		identity = document.getElementById('IconCategory');						// The Icon in Para 1
		IconContent = "<a href='index" +
			CategoryArray[IconCount] + ".php" + 							// Link to 
			"?LogoImage=LogoGrog.jpg&amp;Website=www.animatedknots.com'>" +
			"<img name-'IconImage' border='0' src='indeximages/" + 
			CategoryArray[IconCount] + 
			"top1.png' alt='RollImage'><br>" +								// Selected Image
			TitleArray[IconCount] + "</a>";								// Show Link Name
		identity.innerHTML = IconContent;									// Write to ID
		IconCount++;															// Increment
		if (IconCount > 8) 	{IconCount = 1}								// Maximum = 8
		
		timer = setTimeout("IconRoll()",3000)								// Set Timer 3 Seconds
	}
	// END HIDE-->
</script>




