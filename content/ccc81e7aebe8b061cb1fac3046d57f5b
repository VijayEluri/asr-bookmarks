 <script type="text/javascript">    
     $(document).ready(function(){   
       setFade();  
       setTimeout("setFade()","100");   
     });  
      
    function setFade() {  
         $(".cardimgcrop img").each(function() {    
            if(!$(this).hasClass("hashover") || (!$(this).parents("a").hasClass("hover") && $(this).css("opacity") == "0")) {  
                $(this).unbind();  
                $(this).fadeTo("fast",1);  
                $(this).hover(function(){  $(this).fadeTo(150, 0.0);  
                },function(){ $(this).fadeTo(150, 1.0);  
                });   
                $(this).addClass("hashover");  
            }  
        });  
          
      setTimeout("setFade()","100");   
    }  
    </script>   
 
