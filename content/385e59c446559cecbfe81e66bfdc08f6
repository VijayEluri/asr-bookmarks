<script type="text/javascript" src="http://www.google.com/jsapi?key=ABQIAAAAHZRHD6Q1f0_XG8QssPZNPxRC69y_D-D7B3MmpfjfGc7K7rp5OBSR0e5tC0JH7ZA334eXTjlDcAT7LQ"></script>
<script type="text/javascript">

  // -------------------------------------------
  // Get the Labs Blog posts
  // -------------------------------------------
  if (document.getElementById("news"))
  {
    google.load("feeds", "1");

    function initialize() {
      var feed = new google.feeds.Feed("http://labs.eventful.com/atom.xml");
      feed.load(function(result) {
        if (result.error) {
          // alert("Can't load feed: " + result.error);
        } else {
          var container = document.getElementById("news");
          
          var c = 1;
          
          for (i=0; i<c; i++)
          {
            var entry = result.feed.entries[i];
            if (!entry) 
            {
              continue;
            }
            var div = document.createElement("div");
            div.setAttribute('class', 'news-item');
            var h = document.createElement('h3');
            var a = document.createElement('a');
            a.setAttribute('href', entry.link);
            a.appendChild(document.createTextNode(entry.title));
            h.appendChild(a);
            div.appendChild(h);
            
            var item = document.createElement('div');
            item.setAttribute('class', 'news-item-content');
                        var p = document.createElement('p');
            p.appendChild(document.createTextNode(' '+entry.contentSnippet));
            item.appendChild(p);
                        div.appendChild(item);
            container.appendChild(div);
          }
        }
      });
    }
    google.setOnLoadCallback(initialize);
  }
</script>

