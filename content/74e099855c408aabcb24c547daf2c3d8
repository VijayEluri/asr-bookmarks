            $(document).ready(function() {
                var eCenter = $("#id1292620867183493187");
                var iAll = $("#id1292620867183493187  > div:not(.clear_both)").size();
                var iWidthUnit = $("#id1292620867183493187  > div:not(.clear_both):first").outerWidth({"margin":true});
                var iWidthContainer = eCenter.width();
