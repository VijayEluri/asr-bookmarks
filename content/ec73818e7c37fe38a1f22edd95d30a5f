function MouseOverRoutine(ButtonName)
{
    if (ButtonName=="news") 
        {document.news.src = news_up_a.src;}
    if (ButtonName=="about") 
        {document.about.src = about_up_a.src;}
    if (ButtonName=="people") 
        {document.people.src = people_up_a.src;}
    if (ButtonName=="research") 
        {document.research.src = research_up_a.src;}
    if (ButtonName=="funding") 
        {document.funding.src = funding_up_a.src;}
    if (ButtonName=="publications") 
        {document.publications.src = publications_up_a.src;}
    if (ButtonName=="download") 
        {document.download.src = download_up_a.src;}
    if (ButtonName=="tech_transfer") 
        {document.tech_transfer.src = tech_transfer_up_a.src;}
    if (ButtonName=="spread") 
        {document.spread.src = spread_up_a.src;}
    if (ButtonName=="flush_spread") 
        {document.flush_spread.src = flush_spread_up_a.src;}
    if (ButtonName=="secure_spread") 
        {document.secure_spread.src = secure_spread_up_a.src;}
    if (ButtonName=="backhand") 
        {document.backhand.src = backhand_up_a.src;}
    if (ButtonName=="frugal") 
        {document.frugal.src = frugal_up_a.src;}
    if (ButtonName=="share") 
        {document.share.src = share_up_a.src;}
}

function MouseOutRoutine(ButtonName)
{
    if (ButtonName=="news") 
        {document.news.src = news_up.src;}
    if (ButtonName=="about") 
        {document.about.src = about_up.src;}
    if (ButtonName=="people") 
        {document.people.src = people_up.src;}
    if (ButtonName=="research") 
        {document.research.src = research_up.src;}
    if (ButtonName=="funding") 
        {document.funding.src = funding_up.src;}
    if (ButtonName=="publications") 
        {document.publications.src = publications_up.src;}
    if (ButtonName=="download") 
        {document.download.src = download_up.src;}
    if (ButtonName=="tech_transfer") 
        {document.tech_transfer.src = tech_transfer_up.src;}
    if (ButtonName=="spread") 
        {document.spread.src = spread_up.src;}
    if (ButtonName=="flush_spread") 
        {document.flush_spread.src = flush_spread_up.src;}
    if (ButtonName=="secure_spread") 
        {document.secure_spread.src = secure_spread_up.src;}
    if (ButtonName=="backhand") 
        {document.backhand.src = backhand_up.src;}
    if (ButtonName=="frugal") 
        {document.frugal.src = frugal_up.src;}
    if (ButtonName=="share") 
        {document.share.src = share_up.src;}
}

function MouseDownRoutine(ButtonName)
{
    if (ButtonName=="news") 
        {document.news.src = news_down.src;}
    if (ButtonName=="about") 
        {document.about.src = about_down.src;}
    if (ButtonName=="people") 
