<script type="text/javascript" language="javascript">
function GoogleSearch()
{
	var searchString = document.getElementById('cse-search-box').qtemp.value;
	//searchString = searchString.replace('&','%26');
	
	if (searchString == "") {
		return false;
	} else {
		var replaceq = searchString.split(' ').join('+');
		document.getElementById('cse-search-box').q.value = replaceq;
	}
}
</script>

