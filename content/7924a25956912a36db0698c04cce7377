<!-- Begin: javascript -->
<script type="text/javascript">
function heightAlign() {
  if (document.getElementById) {
    var h;
    var lhs = document.getElementById("maincontent_left");
    var rhs = document.getElementById("primary_sidebar");
    var mco = document.getElementById("maincontent");
    if (lhs && rhs &&  mco) {
      if (lhs.offsetHeight > rhs.offsetHeight) h = lhs.offsetHeight;
      else h = rhs.offsetHeight;
      if (lhs.style) {
        lhs.style.height = h+'px';
        rhs.style.height = h+'px';
        if (mco.offsetHeight < h) {
          h += 5;
          mco.style.height = h+'px';
        }
      }
    }
  }	
}
</script>
<!-- End: javascript -->
