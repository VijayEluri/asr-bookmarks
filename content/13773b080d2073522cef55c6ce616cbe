imgtocache[6] = "/static/images/ticker/big/7.gif";
imgtocache[7] = "/static/images/ticker/big/8.gif";
imgtocache[8] = "/static/images/ticker/big/9.gif";
imgtocache[9] = "/static/images/ticker/big/0.gif";

var args_len = imgtocache.length;
for (var i = args_len; i--;) {
	var cacheImage = document.createElement('img');
	cacheImage.src = imgtocache[i];
	cacheimg.push(cacheImage);
}


function rand ( n ){
  return ( Math.floor ( Math.random ( ) * n + 1 ) );
}
function updateCount(){
	if( !nocount ){
		amt = amt+toadd[toaddcount];
		
		if( toaddcount == 6 )
			toaddcount = 0;
		else
			toaddcount++;
	}
	var amts = number_format(amt, 0, '.', ',');
	amts = amts.toString();
	
	var lnt = amts.length;
	var string;
	var y;
	var num; 
	var ticker = '';
	
	var comm = 1;
	for(x=0;x<lnt;x++){
		y = x+1;
		num = amts.substring(x,y);
		if( num == ',' )
			ticker += '<img src="/static/images/ticker/big/comma.gif">';
		else
			ticker += '<img src="/static/images/ticker/big/'+num+'.gif" hspace=3>';
	}

	jQuery('#count-downloads').html(ticker);
	
	if( amt == 101000000 ) nocount = true;
}

jQuery(document).ready( function(){
	amt = 101000000;
	nocount = true;
	updateCount();
});
