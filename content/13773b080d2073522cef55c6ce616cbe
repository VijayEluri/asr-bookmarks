<script type="text/javascript">
    function validateForm(f) {
    	if(f.signup_level_selected.value) {
    	  return validateStandard(f);
    	} else {
    	 alert("Please select your level.");
         return false;
        }
    }
	function _jsVal_Language() {
	    this.err_form = "Please enter/select values for the following fields:\n\n";
	    this.err_select = "%FIELDNAME%";
	    this.err_enter = "%FIELDNAME%";
	}
	function initValidation(){
	    jsVal_formName = "signup";
	    var f = document.forms[jsVal_formName];
	    jsVal_setRequired(f.email, "Please enter a valid email address.");
	    f.signup_level_selected.value = '';
	}
	initValidation();

	function createPopUp(){
		window.open('create_popup.php?page=1', '','dependent=1,directories=0,height=400,width=450,location=0,scrollbars=1,resizable=1,top=150,left=150');
	}
	function createPopUpForPayment(){
		window.open('create_popup.php?page=2', '','dependent=1,directories=0,height=400,width=450,location=0,scrollbars=1,resizable=1,top=150,left=150');
	}
