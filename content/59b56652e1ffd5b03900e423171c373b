		snaps = ['screenshots/shc-tree.png', 'screenshots/shc-cn2-dist.png', 'screenshots/shc-surveyplot.png', 'screenshots/shc-radviz-intelligent.png', 'screenshots/shc-eval-class.png', 'screenshots/shc-discretization.png', 'screenshots/shc-schema-selection-evaluation.png', 'screenshots/shc-lr-nbc-nomogram.png', 'screenshots/shc-sieve.png', 'screenshots/shc-dendrogram.png', 'screenshots/shc-distance-map.png', 'screenshots/shc-canvas-widget-guess.png', 'screenshots/shc-tree-manual.png', 'screenshots/shc-cn2.png', 'screenshots/shc-canvas-tree-view.png', 'screenshots/shc-misclassifications.png', 'screenshots/shc-tree-exploration.png', 'screenshots/shc-OWNetExplorer-fragviz.png', 'screenshots/shc-tree-navigator.png', 'screenshots/shc-nomogram.png', 'screenshots/shc-freeviz.png', 'screenshots/shc-OWNetExplorer-last.fm.png', 'screenshots/shc-association-rules-filter.png', 'screenshots/shc-som-exploration.png'];
		previm = "screenshots/shc-tree.png";

		function changepict(speed) {
		   do {
			 nextim = snaps[Math.floor(snaps.length * Math.random())];
		   } while (nextim == previm);
		   previm = nextim;
		   swapfade(document.getElementById("img-snap"), nextim, speed, '');  
		}
	</script>
</head>

