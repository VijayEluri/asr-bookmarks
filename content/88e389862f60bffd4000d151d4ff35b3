//photo;size - 0.75

var lastPictNum = GetLastPictNum();
var pictScale   = GetPictScale();
var arrImgNum = new Array(999,999,999,999,999,999);

function ShowPhoto(){

	document.phM1.src = GetPreviewUrl(StartRandom(0));
	document.phM2.src = GetPreviewUrl(StartRandom(1));
	document.phM3.src = GetPreviewUrl(StartRandom(2));
	document.phM4.src = GetPreviewUrl(StartRandom(3));
	document.phM5.src = GetPreviewUrl(StartRandom(4));
	document.phM6.src = GetPreviewUrl(StartRandom(5));

}

var nTemp, nLoop;

function StartRandom(nIndex){

	nLength = Number(lastPictNum);
	nLoop   = 0;
	nTemp   = Math.round(Math.random() * (nLength - 1));

	if(nIndex != 0)
	{
		for(nLoop = 0; nLoop < nIndex ; nLoop++)
		{
			if(nTemp == arrImgNum[nLoop])
			{
				StartRandom(nIndex);
			}
		}
	}
	arrImgNum[nIndex] = nTemp;

	return nTemp;
}

function uAction(n){

	location.href = "target.php?id=" + String(arrImgNum[n - 1]);

}

