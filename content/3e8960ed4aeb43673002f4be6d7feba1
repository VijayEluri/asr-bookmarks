$('sstarget'+pl).appear({duration:.1,queue:{scope:'slideshow',limit: 4}});
}
if ((pl+1)>=numslides) {ns = 1;} else {ns = pl+1;}
pe = new PeriodicalExecuter(setimg,5);
}
function setimg() {
$('sstarget'+slide).fade({duration:2,queue:{scope:'slideshow',limit: 4}});
