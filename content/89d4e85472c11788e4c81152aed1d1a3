							function sortGenderElements(elem) {
								
								//init
								var options = elem.childElements();
								var gender = formatGender(options[elem.selectedIndex].text);
								
								//sort out carousel
								$('galleryList').select('li').each(function(item) {
									if (item.hasClassName('gender') && item.hasClassName('gender' + gender)) {
										$('galleryList').insert({
											top: item.remove()
										});
									} else if (item.hasClassName('gender') && !item.hasClassName('gender' + gender)) {
										$('galleryList').insert({
											bottom: item.remove()
										});
									}
								});
								
								Snorg.setCarouselActive();
								Snorg.hCarousel.scrollTo(0);
								
