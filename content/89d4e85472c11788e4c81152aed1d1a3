								if($(gender)) {
									tabs.setActiveTab(gender);
								}
																
								//sort out sizing chart
								$$('p.size-chart').each(function(link) {
									if (link.hasClassName('hide') && link.id == gender.toLowerCase() + '-size-chart') {
										link.removeClassName('hide');
									} else {
										link.addClassName('hide');									
									}
								});
								
							}
							
							$$('select.super-attribute-select').each(function(curElem) {
								var label = curElem.previous().innerHTML;
	                            var options = curElem.childElements();
	                            //options[0].update('Choose ' + label);
	                            options[0].setAttribute('selected', 'selected');
					        	if (label==='Gender') {
					        		Event.observe(curElem, 'change', function() { 
					        			sortGenderElements(curElem);
									});
								}
					        	Snorg.fireEvent(curElem, 'change');
							});
							
							function formatGender(str) {
								var plusTest = str.indexOf('+');
								var minusTest = str.indexOf('-');
								
								if (plusTest!='-1') {
									var gender = str.substr(0,plusTest);
								} else if (minusTest!='-1') {
									var gender = str.substr(0,minusTest);
								} else {
									var gender = str;
								}
								
								return gender.strip();
							
							}
	
						</script>
							
					<input id="addToCart" onclick="productAddToCartForm.submit(); return false;" type="image" class="button" src="http://www.snorgtees.com/skin/frontend/default/200906_snorg/images/btn-add.gif" alt="ADD TO CART" />
				</fieldset>
			</form>
			<script type="text/javascript">
			//<![CDATA[
			    var productAddToCartForm = new VarienForm('product_addtocart_form');
